{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "ordered-imports": [true, {
    "import-sources-order": "any",
    "named-imports-order": "any"
  }],
  "rules": {
    "no-console": false,
    "interface-name" : [true, "never-prefix"]
  }
}
